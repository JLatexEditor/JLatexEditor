# TexShop on Mac OS X

# File menu
shortcut.new=meta N
shortcut.open=meta O
shortcut.open\ last\ closed=meta shift W
shortcut.save=meta S
shortcut.save\ as=meta shift S
shortcut.close=meta W
shortcut.exit=meta Q
# Edit menu
shortcut.undo=meta Z
shortcut.redo=meta shift Z
shortcut.find=meta F
shortcut.replace=meta R
shortcut.find\ next=meta G
shortcut.find\ previous=shift meta G
shortcut.cut=meta X
shortcut.copy=meta C
shortcut.paste=meta V
shortcut.select\ all=meta A
shortcut.select\ none=meta shift A
shortcut.comment=shift meta OPEN_BRACKET
shortcut.uncomment=shift meta CLOSE_BRACKET
#shortcut.diff=alt D
# View

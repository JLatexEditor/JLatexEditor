
<commandList>
  <command name="item" usage="\begin{itemize}\n  \item @|@\n\end{itemize}" hint="itemize environment">
  </command>
  <command name="enum" usage="\begin{enumerate}\n  \item @|@\n\end{enumerate}" hint="enumerate environment">
  </command>
  <command name="tikz" usage="\begin{tikzpicture}\n  @|@\n\end{tikzpicture}" hint="tikz picture environment">
  </command>
  <command name="fig" usage="\begin{figure}\n  @|@\n\end{figure}" hint="figure environment">
  </command>
  <command name="center" usage="\begin{center}\n  @|@\n\end{center}" hint="center environment">
  </command>
  <command name="align" usage="\begin{align}\n  @|@\n\end{align}" hint="align environment">
  </command>
  <command name="align*" usage="\begin{align*}\n  @|@\n\end{align*}" hint="align environment">
  </command>
  <command name="sec" usage="\section{@name@}\label{sec:@label@}\n@|@" hint="section">
    <argument name="name">
    </argument>
    <argument name="label">
    </argument>
  </command>
  <command name="ssec" usage="\subsection{@name@}\label{sec:@label@}\n@|@" hint="subsection">
    <argument name="name">
    </argument>
    <argument name="label">
    </argument>
  </command>
</commandList>
